
public class PrimeNo {

	public static void main(String[] args) {
	int i, m=0, flag=0;
	int n=5; // change prime number according to you 
	m=n/2;
	if(n==0||n==1)
	{
		System.out.println("not prime number");
	}
	else 
	{
		for(i=2;i<=m;i++)
		{
			if(n%2==0)
			{
				System.out.println("not prime number");
				flag=1;
				break;
				
			}
			else 
			{
				if(flag==0)
				{
					System.out.println("prime number");
				}
			}
		}
		}
		
	}
	}


